{
  "name": "minesweeper-tg",
  "version": "1.0.0",
  "private": true,
  "type": "module",  "scripts": {
    "dev": "npm run dev:client & npm run dev:server",
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "build": "cd client && vite build",
    "install:deps": "npm install && cd client && npm install && cd ../server && npm install",
    "vercel-build": "npm run install:deps",
    "start": "cd server && npm install && node index.js"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.13",
    "@vitejs/plugin-react": "^4.2.1",
    "express": "^4.18.2",
    "lucide-react": "^0.509.0",
    "nanoid": "^5.1.5",
    "vite": "^6.3.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "gh-pages": "^6.3.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
